{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/jobs",
      "dest": "/api/jobs.js"
    },
    {
      "src": "/api/career/apply",
      "dest": "/api/career.js"
    },
    {
      "src": "/api/career/update-status",
      "dest": "/api/career/update-status.js"
    },
    {
      "src": "/api/career/debug-properties",
      "dest": "/api/career/debug-properties.js"
    },
    {
      "src": "/api/application-detail",
      "dest": "/api/application-detail.js"
    },
    {
      "src": "/api/applications",
      "dest": "/api/applications.js"
    },
    {
      "src": "/api/applications/get-single",
      "dest": "/api/applications/get-single.js"
    },
    {
      "src": "/api/applications/first-interview",
      "dest": "/api/applications/first-interview.js"
    },
    {
      "src": "/api/applications/final-interview",
      "dest": "/api/applications/final-interview.js"
    },
    {
      "src": "/api/applications/rejected",
      "dest": "/api/applications/rejected.js"
    },
    {
      "src": "/api/applications/selected",
      "dest": "/api/applications/selected.js"
    },
    {
      "src": "/api/applications/hired",
      "dest": "/api/applications/hired.js"
    },
    {
      "src": "/api/applications/interview",
      "dest": "/api/applications/interview.js"
    },
    {
      "src": "/api/applications/update-questions",
      "dest": "/api/applications/update-questions.js"
    },
    {
      "src": "/api/applications/update-status",
      "dest": "/api/applications/update-status.js"
    }
  ]
}